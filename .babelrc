{
   "presets": [
       [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": "last 2 versions"
                },
                "modules": false,
                "loose": false
            }
        ],
       ["@babel/preset-react", {"runtime": "automatic"}]
   ],
   "plugins": [
     ["@babel/transform-runtime"],
     ["babel-plugin-styled-components"]
  ]
}